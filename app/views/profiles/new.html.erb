<h3> Please complete your user profile </h3>
<%= simple_form_for(article) do |f| %>
  <%= f.input :first_name%>
  <%= f.input :last_name%>
  <%= f.input :email%>
  <%= f.input :background%>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.input :resume %>
  <%= f.input :resume_cache, as: :hidden %>
<% end %>


