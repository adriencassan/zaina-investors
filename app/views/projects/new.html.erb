<h2>Create a new project, next : should pop up from a button in tabs header</h2>

<%= simple_form_for @project do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :project_name, required: true %>
    <%= f.input :description, required: true %>
    <%= f.input :sector, required: true %>
  </div>

 <div class="actions">
    <%= f.submit "Submit project" %>
  </div>
<% end %>
